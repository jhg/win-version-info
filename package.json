{
  "name": "win-version-info",
  "version": "2.0.0",
  "description": "Windows-only native addon to read version info from executables",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "win-version-info": "bin.js",
    "version-info": "bin.js"
  },
  "author": "Vincent Weevers",
  "files": [
    "bin.js",
    "index.js",
    "skip.js",
    "binding.gyp",
    "src",
    "deps"
  ],
  "scripts": {
    "test": "tape test/*-test.js && prebuild-ci",
    "install": "node skip.js || prebuild-install || node-gyp rebuild",
    "rebuild": "prebuild --compile"
  },
  "dependencies": {
    "bindings": "~1.3.0",
    "nan": "~2.10.0",
    "prebuild-install": "~4.0.0"
  },
  "devDependencies": {
    "prebuild": "~7.6.0",
    "prebuild-ci": "~2.2.3",
    "tape": "~4.9.0",
    "win-dummy-exe": "0.0.1",
    "xtend": "~4.0.1"
  },
  "keywords": [
    "exe",
    "executable",
    "productname",
    "productversion",
    "version",
    "versioninfo",
    "windows"
  ],
  "engines": {
    "node": ">=6"
  },
  "gypfile": true,
  "repository": "vweevers/win-version-info",
  "bugs": "https://github.com/vweevers/win-version-info/issues",
  "homepage": "https://github.com/vweevers/win-version-info"
}
